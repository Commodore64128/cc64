
; cc64 runtime module start
; to run on c16/plus4 at $1001-$8000

!set    cc64_statics_start = $8000
!addr   cc64_statics_end   = $8000
!addr   cc64_frameptr      = $22
!addr   cc64_zp            = $24

!addr   tmp_zp1            = $1e
!addr   tmp_zp2            = $20

!addr   strout    = $9088
!addr   warmstart = $8003

+cc64_runtime_start $1001

cc64_lib_start
        !word baslink
        !word 2020
        !byte $9e
        !pet " 4129:"
        !byte $8f
        !pet " cc64 rt-lib v0.6", 0
baslink !word 0

        jmp init

division_by_zero
        lda #<div_zero
        ldy #>div_zero
        jsr strout
        jmp warmstart
div_zero !pet "division by zero", $0d, 0
